{
  "version": 2,
  "buildCommand": "cd apps/web && npm install && npm run build",
  "outputDirectory": "apps/web/dist",
  "functions": {
    "server/src/index.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server/src/index.js"
    }
  ],
  "env": {
    "GEMINI_MODEL": "gemini-2.0-flash-exp"
  }
}
